{
  "name": "baogame",
  "description": "a html5 mutiplayer game",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/baogame.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/baogame/issues"
  },
  "scripts": {
    "start": "node dist/app.js",
    "debug": "node dist/app.js --debug",
    "build": "npm run clean && npm run file2variable && rev-static --config static/rev-static.image.js && tsc -p src/back/ && tsc -p src/front/ && webpack --config static/webpack.config.js && rev-static --config static/rev-static.config.js",
    "clean": "rimraf dist/ static/dist/ static/scripts/ \"static/imgs/**/*-*.png\"",
    "lint": "tslint \"src/**/*.ts\"",
    "file2variable": "file2variable-cli static/protocol.proto src/front/template.html -o src/front/proto-variables.ts --protobuf --html-minify"
  },
  "engines": {
    "node": ">=6"
  },
  "homepage": "https://github.com/plantain-00/baogame#readme",
  "dependencies": {
    "express": "4.15.3",
    "minimist": "1.2.0",
    "protobufjs": "6.7.3",
    "reconnection": "1.0.0",
    "uws": "0.14.5"
  },
  "devDependencies": {
    "@types/express": "4.0.35",
    "@types/jquery": "2.0.45",
    "@types/minimist": "1.2.0",
    "@types/uws": "0.13.0",
    "file2variable-cli": "1.3.0",
    "rev-static": "2.4.2",
    "rimraf": "2.6.1",
    "tslib": "1.7.1",
    "tslint": "5.4.0",
    "typescript": "2.4.0",
    "vue": "2.3.3",
    "vue-class-component": "5.0.1",
    "webpack": "2.6.1"
  }
}
